{
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html",
                    "/*.css",
                    "/*.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/**"
                ],
                "urls": [
                    "https://unpkg.com/onsenui/css/**"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "backend",
            "urls": [
                "/customers"
            ],
            "cacheConfig": {
                "strategy": "freshness",
                "maxSize": 15,
                "maxAge": "1h",
                "timeout": "5s"
            }
        }
    ]
}